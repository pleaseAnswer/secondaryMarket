.picker-box-bg {
  background: #000;
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  opacity: 0.6;
  z-index: 99;
}
.picker-container {
  width: 100%;
  max-height: 60%;
  overflow: scroll;
  position: fixed;
  bottom: 0;
  background: #fff;
  padding-bottom: 20rpx;
  border-radius: 15rpx 15rpx 0 0;
  z-index: 100;
}
.picker-title {
  width: calc(100% - 80rpx);
  padding: 20rpx 0;
  margin: 0 40rpx;
  color: #58b;
  box-sizing: border-box;
  position: fixed;
  background: #fff;
  display: flex;
  justify-content: space-between;
}
.picker-title-cancel {
  color: #ccc;
}
.picker-title-text {
  color: #000;
}
.picker-title-sure {
  color: #58bc;
}
.pick-item-wrap {
  margin-top: 100rpx;
}
.pick-item-wrap view {
  width: 100%;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
  text-align: center;
}